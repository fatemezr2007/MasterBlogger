@page
@model MB.Presentation.MVCCore.Areas.Administrator.Pages.ArticleCateguryManagement.EditModel
@{
}

<div class="card">
    <div class="card-header">
        <h2 class="card-title">Edit Article Categury : @Model.ArticleCategury.Title</h2>
    </div>
    <div class="card-body">
        <form method="post">
            <input type="hidden" asp-for="ArticleCategury.Id" /> 

            <div class="form-group">
                <label asp-for="ArticleCategury.Title" class="form-label">Title: </label>
                <input asp-for="ArticleCategury.Title" class="form-control" />
            </div>

            <div class="form-group button-group">
                <button type="submit" class="btn btn-primary">Edit</button>
                <a asp-page="/Administrator/ArticleCategoryManagement/List" class="btn btn-secondary">Back to list</a>
            </div>
        </form>
    </div>
</div>